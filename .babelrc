{
  "presets": [
    "next/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        "inline-dotenv",
        "transform-decorators-legacy",
        "transform-class-properties",
        ["styled-components", {
          "ssr": true,
          "displayName": true,
          "preprocess": false,
          "minify" : false
        }],
        ["import", {
          "libraryName": "antd",
          "style": "css"
        }]
      ]
    },
    "production": {
      "plugins": [
        "inline-dotenv",
        "transform-decorators-legacy",
        "transform-class-properties",
        ["styled-components", {
          "ssr": true,
          "displayName": false,
          "preprocess": false,
          "minify" : true
        }],
        ["import", {
          "libraryName": "antd",
          "style": "css"
        }]
      ]
    }
  }
}
